{
  "backgroundImage": "https://source.unsplash.com/1920x1080/nature",
  "webTitle": "我的导航站",
  "favicon": "./icons/favicon.ico",
  "settings": {
    "timeSeconds": true,
    "brightness": 80,
    "blur": 0,
    "fontColor": "#ffffff",
    "searchRadius": 50,
    "clockAreaHeight": 245,
    "linkOpen": "_blank",
    "cardStyle": {
      "widthNum": 160,
      "heightNum": 64,
      "radius": 16,
      "gap": 20,
      "boxShadow": "0 4px 16px rgba(0, 0, 0, 0.1)"
    }
  },
  "searchEngines": [
    {
      "label": "Google",
      "link": "https://www.google.com/search?q=",
      "icon": "./icons/google.svg",
      "use": true
    },
    {
      "label": "百度",
      "link": "https://www.baidu.com/s?wd=",
      "icon": "./icons/baidu.svg",
      "use": true
    },
    {
      "label": "Bing",
      "link": "https://www.bing.com/search?q=",
      "icon": "./icons/bing.svg",
      "use": true
    },
    {
      "label": "DuckDuckGo",
      "link": "https://duckduckgo.com/?q=",
      "icon": "./icons/duckduckgo.svg",
      "use": true
    }
  ],
  "aiChat": {
    "enabled": true,
    "url": "https://chatgpt.com",
    "title": "AI对话",
    "icon": "https://example.com/ai-icon.svg",
    "window": {
      "width": 1024,
      "height": 768,
      "position": "left-center",
      "minWidth": 300,
      "minHeight": 400
    }
  }
}